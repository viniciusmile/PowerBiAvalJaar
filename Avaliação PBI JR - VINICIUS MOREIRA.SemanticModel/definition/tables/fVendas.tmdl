	measure 'Média de vendas brutas diárias' =
			
			AVERAGEX (
			    VALUES (dCalendario[Data]),
			    CALCULATE ([Vendas Brutas])
			)
		formatString: "R$"\ #,0;-"R$"\ #,0;"R$"\ #,0

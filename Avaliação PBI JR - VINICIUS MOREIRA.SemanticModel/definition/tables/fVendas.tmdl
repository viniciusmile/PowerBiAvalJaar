	measure 'Vendas brutas LY' =
			
			VAR VendasAtuais = [Vendas Brutas]
			VAR VendasLY =
			    CALCULATE(
			        [Vendas Brutas],
			        SAMEPERIODLASTYEAR( 'dCalendario'[Data] )
			    )
			RETURN
			    IF(
			        NOT ISBLANK( VendasAtuais ),
			        VendasLY,
			        BLANK()
			    )
		lineageTag: d2dc476c-25e2-40ea-b321-edae932da66f

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure '% Vendas brutas LY' =
			
			DIVIDE(
			    [Vendas Brutas] - [Vendas brutas LY],
			    [Vendas brutas LY]
			)
		formatString: 0.0%;-0.0%;0.0%

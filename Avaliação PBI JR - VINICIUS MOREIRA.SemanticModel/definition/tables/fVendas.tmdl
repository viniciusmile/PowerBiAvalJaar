	measure 'Vendas brutas YTD' =
			
			TOTALYTD(
			    [Vendas Brutas],
			    dCalendario[Data]
			)
		formatString: "R$"\ #,0.###############;-"R$"\ #,0.###############;"R$"\ #,0.###############
		lineageTag: 7a650236-4303-4fb0-a891-2f14588002be

		annotation PBI_FormatHint = {"currencyCulture":"pt-BR"}

	measure 'Vendas brutas LY Exato' =
			
			
			// 1. Declarar TODAS as variáveis primeiro
			VAR UltimaDataVendaNoContexto =
			    MAX ( fVendas[Data] )
			
			VAR DataInicioPeriodoAtual =
			    MIN ( 'dCalendario'[Data] )
			
			VAR PeriodoLYEquivalente =
			    DATESBETWEEN (
			        'dCalendario'[Data],
			        EDATE ( DataInicioPeriodoAtual, -12 ), // Data de início LY
			        EDATE ( UltimaDataVendaNoContexto, -12 ) // Data de fim LY (parcial)
			    )
			
			// 2. Agora, usar o RETURN com a lógica IF
			RETURN
			IF (
			    // Se não houver vendas no mês atual (ex: um mês futuro), não mostre nada.
			    ISBLANK ( UltimaDataVendaNoContexto ),
			    BLANK (),
			
			    // Se houver vendas, calcule o período LY equivalente
			    CALCULATE ( [Vendas Brutas], PeriodoLYEquivalente )
			)
		formatString: #,0
		lineageTag: 4e072d87-9b24-41cc-901a-c7d33195f3d2

	measure '% Share vendas brutas Categoria Produtos' =
			
			DIVIDE(
			    [Vendas Brutas],
			    CALCULATE(
			        [Vendas Brutas],
			        ALL( 'dCategoriaProdutos' )
			    )
			)
		formatString: 0.0%;-0.0%;0.0%
		lineageTag: c0310082-b316-4368-9803-38d8f32c7b5b

	measure '% Share vendas brutas Genérico' =
			
			DIVIDE(
			    [Vendas Brutas],
			    CALCULATE(
			        [Vendas Brutas],
			        ALL()
			    )
			)
		formatString: 0.0%;-0.0%;0.0%
		lineageTag: 23b811dc-b782-4652-a136-a0a06e914b11

	measure '% Share vendas brutas Agrupável' =
			
			VAR Numerador = [Vendas Brutas]
			VAR Denominador =
			    IF(
			        ISINSCOPE( 'dCategoriaProdutos'[Categoria] ),
			        -- Se estiver no nível Categoria, o denominador é o total do Continente
			        CALCULATE(
			            [Vendas Brutas],
			            ALL( 'dCategoriaProdutos' )
			        ),
			        -- Se estiver no nível Continente, o denominador é o Total Geral
			        CALCULATE(
			            [Vendas Brutas],
			            ALL( 'dLocalidades' )
			        )
			    )
			RETURN
			    DIVIDE( Numerador, Denominador )
		formatString: 0.0%;-0.0%;0.0%
		lineageTag: fadadacc-65cd-42d0-b9f1-609548307939

	measure 'Ranking promoções' = ```
			
			IF(
			    ISINSCOPE( 'dPromocoes'[Promocao] ),
			    
			    // 1. Captura as vendas da linha atual no visual
			    VAR VendasLinhaAtual = [Vendas líquidas]
			    
			    // 2. Cria uma tabela virtual de TODAS as promoções (agrupadas por nome) 
			    //    e calcula as Vendas Líquidas para cada uma
			    VAR TabelaResumo =
			        ADDCOLUMNS(
			            ALL( 'dPromocoes'[Promocao] ),
			            "@Vendas", [Vendas líquidas]
			        )
			        
			    // 3. Conta quantas promoções na tabela virtual têm vendas MAIORES que a linha atual
			    VAR Ranking =
			        COUNTROWS(
			            FILTER(
			                TabelaResumo,
			                [@Vendas] > VendasLinhaAtual
			            )
			        ) + 1 // Adiciona 1 para obter o ranking
			        
			    RETURN
			        Ranking,
			        
			    BLANK() // Retorna vazio para a linha Total
			)
			```
		formatString: 0
		lineageTag: 9873b2b3-2cad-465b-bad0-c734bd9b404d

